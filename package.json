    "dev": "nx run-many -t serve -p chat-ui chat-api",
    "build": "nx run-many -t build -p chat-ui chat-api",
    "chat-ui:generate:client": "java -jar open-api-generator-cli/openapi-generator-cli.jar generate -i apps/chat-ui/src/server/chat-api/chat-api.yaml -g typescript-fetch -o apps/chat-ui/src/server/openapi-client --additional-properties=supportsES6=true,typescriptThreePlus=true,npmVersion=1.0.0,withInterfaces=true",
    "ecommerce-assistant-ui-ui:generate:client": "java -jar open-api-generator-cli/openapi-generator-cli.jar generate -i apps/ecommerce-assistant-ui/src/server/ecommerce-api/ecommerce-assistant-api.yaml -g typescript-fetch -o apps/ecommerce-assistant-ui/src/server/openapi-client --additional-properties=supportsES6=true,typescriptThreePlus=true,npmVersion=1.0.0,withInterfaces=true"
